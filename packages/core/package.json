{
  "name": "@predable/core",
  "version": "1.0.0",
  "description": "Predable is type only state management framework.",
  "keywords": [
    "predable",
    "typescript",
    "state",
    "management",
    "type"
  ],
  "homepage": "https://github.com/azu/predable/tree/master/packages/core/",
  "bugs": {
    "url": "https://github.com/azu/predable/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/predable.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/core.js",
  "types": "lib/core.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "dependencies": {
    "composite-symbol": "^1.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.1",
    "cross-env": "^7.0.2",
    "mocha": "^7.1.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.1",
    "ts-node-test-register": "^8.0.1",
    "typescript": "^3.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
